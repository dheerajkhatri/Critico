upports xml, json, xmlrpc, jsonrpc, amfrpc, rss, csv\n    """\n    return service()\n\n\n@auth.requires_login() \ndef api():\n    """\n    this is example of API with access control\n    WEB2PY provides Hypermedia API (Collection+JSON) Experimental\n    """\n    from gluon.contrib.hypermedia import Collection\n    rules = {\n        \'<tablename>\': {\'GET\':{},\'POST\':{},\'PUT\':{},\'DELETE\':{}},\n        }\n    return Collection(db).process(request,response,rules)\n\ndef getRegistrationForm():\n    for field in [\'gender\', \'date_of_birth\', \'country\', \'state_\', \'city\', \'display_picture_name\', \'display_picture_blob\']:\n        db.auth_user[field].readable = db.auth_user[field].writable = False\n    for field in [\'user_type\']:\n        db.auth_user[field].readable = db.auth_user[field].writable = True\n    form = auth.register()\n    if(form.accepts(request, session)):\n        if(form.vars.user_type == \'User\'):\n            print form.vars.id\n    return auth.register()\nresponse._vars=response._caller(user)\n'
