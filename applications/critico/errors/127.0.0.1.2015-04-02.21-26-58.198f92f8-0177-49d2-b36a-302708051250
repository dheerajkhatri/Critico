S'# -*- coding: utf-8 -*-\n# this file is released under public domain and you can use without limitations\n\n#########################################################################\n## This is a sample controller\n## - index is the default action of any application\n## - user is required for authentication and authorization\n## - download is for downloading files uploaded in the db (does streaming)\n## - api is an example of Hypermedia API support and access control\n#########################################################################\n\nINDEX_PAGE=\'index\'\nPRODUCT_FORM_PAGE=\'product_manager\'\nEDITION_MANAGER_PAGE=\'edition_manager\'\nEDITION_MANAGER_EDIT_PAGE=\'edition_manager_edit\'\nSUB_EDITION_MANAGER_EDIT_PAGE=\'sub_edition_manager_edit\'\n\n@auth.requires_login()\ndef index():\n    """\n    example action using the internationalization operator T and flash\n    rendered by views/default/index.html or views/generic.html\n\n    if you need a simple wiki simply replace the two lines below with:\n    return auth.wiki()\n    """\n    userId = session.auth.user.id\n    userType = getAuthUserType(userId)\n    if userType == \'a\':\n        return userIndex()\n    elif userType == \'b\':\n        return productIndex(userId)\n\ndef userIndex():\n    return dict(message=\'Hi user\')\n\ndef productIndex(adminId):\n    form = SQLFORM.factory(submit_button=\'Update Product Description\')\n    form.add_button(\'Add/Edit Editions\', URL(EDITION_MANAGER_PAGE))\n    if form.process().accepted:\n        redirect(URL(PRODUCT_FORM_PAGE))\n    return dict(message=\'Hi product admin\', form=form)\n\ndef product_manager():\n    adminId = session.auth.user.id\n    prodId = getProductId(adminId)\n    return dict(form=getProductManagerEdit(prodId))\n\ndef edition_manager():\n    adminId = session.auth.user.id\n    prodId = getProductId(adminId)\n    return dict(form=getEditionManagerListEdit(prodId))\n\ndef edition_manager_edit():\n    edId = request.vars.ed\n    adminId = session.auth.user.id\n    prodId = getProductId(adminId)\n    if edId == \'\':\n        edId = -1\n    else:\n        edId = int(edId)\n    return dict(form=getEditionManagerEdit(edId, prodId))\n\ndef sub_edition_manager_edit():\n    edId = int(request.vars.ed)\n    subedId = request.vars.subed\n    adminId = session.auth.user.id\n    prodId = getProductId(adminId)\n\n    ## Some checking\n\n    if subedId == \'\':\n        subedId = -1\n    else:\n        subedId = int(subedId)\n    return dict(form=getSubEditionManagerEdit(subedId, edId, prodId))\n\ndef user():\n    """\n    exposes:\n    http://..../[app]/default/user/login\n    http://..../[app]/default/user/logout\n    http://..../[app]/default/user/register\n    http://..../[app]/default/user/register_user\n    http://..../[app]/default/user/register_product_admin\n    http://..../[app]/default/user/profile\n    http://..../[app]/default/user/retrieve_password\n    http://..../[app]/default/user/change_password\n    http://..../[app]/default/user/manage_users (requires membership in\n    use @auth.requires_login()\n        @auth.requires_membership(\'group name\')\n        @auth.requires_permission(\'read\',\'table name\',record_id)\n    to decorate functions that need access control\n    """\n    if request.args(0) == \'register_user\':\n        return dict(form=getUserRegistrationForm())\n    elif request.args(0) == \'register_product_admin\':\n        return dict(form=getSoapAdminRegistrationForm())\n    elif request.args(0) == \'profile\':\n        return dict(form=getProfileForm())\n    return dict(form=auth())\n\n@cache.action()\ndef download():\n    """\n    allows downloading of uploaded files\n    http://..../[app]/default/download/[filename]\n    """\n    return response.download(request, db)\n\n\ndef call():\n    """\n    exposes services. for example:\n    http://..../[app]/default/call/jsonrpc\n    decorate with @services.jsonrpc the functions to expose\n    supports xml, json, xmlrpc, jsonrpc, amfrpc, rss, csv\n    """\n    return service()\n\n\n@auth.requires_login() \ndef api():\n    """\n    this is example of API with access control\n    WEB2PY provides Hypermedia API (Collection+JSON) Experimental\n    """\n    from gluon.contrib.hypermedia import Collection\n    rules = {\n        \'<tablename>\': {\'GET\':{},\'POST\':{},\'PUT\':{},\'DELETE\':{}},\n        }\n    return Collection(db).process(request,response,rules)\n\ndef updateAuthUserUserType(userId, userType):\n    db(db.auth_user.id == userId).update(user_type = userType)\n\ndef makeAuthUSerUser(userId):\n    updateAuthUserUserType(userId, \'a\')\n\ndef makeAuthUSerSoapAdmin(userId):\n    updateAuthUserUserType(userId, \'b\')\n\ndef approveAuthUser(userId):\n    db(db.auth_user.id == userId).update(registration_key=\'\')\n\ndef getUserRegistrationForm():\n    form = getRegistrationForm()\n    if form.accepts(request, session):\n        makeAuthUSerUser(form.vars.id)\n        approveAuthUser(form.vars.id)\n    return form\n\ndef getSoapAdminRegistrationForm():\n    form = getRegistrationForm()\n    if form.accepts(request, session):\n        makeAuthUSerSoapAdmin(form.vars.id)\n        approveAuthUser(form.vars.id)   ## ------------------- This line will be removed in production\n    return form\n\ndef disalbeAuthFieldReadOrWrite(fieldList):\n    for field in fieldList:\n        db.auth_user[field].readable = db.auth_user[field].writable = False\n\ndef getRegistrationForm():\n    disalbeAuthFieldReadOrWrite([\'gender\', \'date_of_birth\', \'country\', \'state_\', \'city\', \'display_picture_name\', \'display_picture_blob\'])\n    return auth.register()\n\n@auth.requires_login()\ndef getProfileForm():\n    disalbeAuthFieldReadOrWrite([\'username\'])\n    return auth.profile()\n\ndef getAuthUserType(userId):\n    return db(db.auth_user.id == userId).select()[0].user_type\n\n\n\'\'\'\nProduct Functions\nStart\n\'\'\'\ndef insertProduct(adminId, prodName, picName, picBlob):\n    return db.product.insert(admin_ref=adminId, name=prodName, display_picture_name=picName, display_picture_blob=picBlob)\n\ndef initProduct(adminId):\n    return insertProduct(adminId, \'\', \'\', \'\')\n\ndef getProductId(adminId):\n    val = isProductWithThisAdminIdExist(adminId)\n    if val == -1:\n        return initProduct(adminId)\n    else:\n        return val\n\ndef isProductWithThisAdminIdExist(adminId):\n    rows = db(db.product.admin_ref == adminId).select()\n    if(not rows):\n        return -1\n    else:\n        return rows[0].id\n\ndef updateProductName(prodId, prodName):\n    db(db.product.id == prodId).update(name=prodName)\n\ndef updateProductPic(prodId, picName, picBlob):\n    db(db.product.id == prodId).update(display_picture_name=picName, display_picture_blob=picBlob)\n\ndef getProductManagerEdit(prodId):\n    update = db.product(prodId)\n    form = SQLFORM(db.product, update, showid=False, upload=URL(r=request,f=\'download\'))\n    form.add_button(\'Back\', URL(INDEX_PAGE))\n    if form.accepts(request, session):\n        redirect(URL(PRODUCT_FORM_PAGE))\n    return form\n\n\'\'\'\nProduct Functions\nEnd\n\'\'\'\n\ndef updateEditionProductId(edId, prodId):\n    db(db.edition.id == edId).update(product_ref=prodId)\n\ndef isEditionWithThisIdExist(edId):\n    if not db(db.edition.id == edId).select():\n        return False\n    else:\n        return True\n\ndef getEditionManagerListEdit(prodId):\n    form = SQLFORM.factory(submit_button=\'Add edition\')\n    form.add_button(\'Back\', URL(INDEX_PAGE))\n    rows = db(db.edition.product_ref == prodId).select()\n    for row in rows:\n        str_=(\'Edit Edition #\' + str(row.number_))\n        form.add_button(str_, URL(EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=str(row.id))))\n    if form.accepts(request, session):\n        redirect(URL(EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=\'\')))\n    return form\n\ndef getEditionManagerEdit(edId, prodId):\n    update = db.edition(edId)\n    form = SQLFORM(db.edition, update, showid=False, upload=URL(r=request,f=\'download\'))\n    form.add_button(\'Back\', URL(EDITION_MANAGER_PAGE))\n\n    ## for sub editions\n    rows = db(db.sub_edition.product_ref == edId).select()\n    for row in rows:\n        str_=(\'Edit Sub Edition #\' + str(row.number_))\n        form.add_button(str_, URL(SUB_EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=str(edId), subed=str(row.id))))\n    if isEditionWithThisIdExist(edId):\n        form.add_button(\'Add sub edition\', URL(SUB_EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=str(edId), subed=\'\')))\n    ## -----\n    if form.accepts(request, session):\n        updateEditionProductId(form.vars.id, prodId)\n    return form\n\n\ndef updateSubEditionProductId(subedId, prodId):\n    db(db.edition.id == subedId).update(product_ref=prodId)\n\ndef updateSubEditionEditionId(subedId, edId):\n    db(db.edition.id == subedId).update(edition_ref=edId)\n\ndef getSubEditionManagerEdit(subedId, edId, prodId):\n    update = db.sub_edition(subedId)\n    form = SQLFORM(db.sub_edition, update, showid=False, upload=URL(r=request,f=\'download\'))\n    form.add_button(\'Back\', URL(EDITION_MANAGER_PAGE,  vars=dict(ed=str(edId))))\n\n    ## for videos\n    \'\'\'\n    rows = db(db.sub_edition.product_ref == edId).select()\n    for row in rows:\n        str_=(\'Edit Sub Edition #\' + str(row.number_))\n        form.add_button(str_, URL(SUB_EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=str(edId), subed=str(row.id))))\n    if isEditionWithThisIdExist(edId):\n        form.add_button(\'Add sub edition\', URL(SUB_EDITION_MANAGER_EDIT_PAGE, vars=dict(ed=str(edId), subed=\'\')))\n    \'\'\'\n    ## -----\n    if form.accepts(request, session):\n        updateSubEditionProductId(form.vars.id, prodId)\n        updateSubEditionEditionId(form.vars.id, edId)\n    return form\nresponse._vars=response._caller(sub_edition_manager_edit)\n'
